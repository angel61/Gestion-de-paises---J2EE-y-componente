<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ServletRA6</servlet-name>
        <servlet-class>ServletRA6</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlMunicipio</servlet-name>
        <servlet-class>control.ControlMunicipio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlPais</servlet-name>
        <servlet-class>control.ControlPais</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlBD</servlet-name>
        <servlet-class>control.ControlBD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlVivienda</servlet-name>
        <servlet-class>control.ControlVivienda</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlPersona</servlet-name>
        <servlet-class>control.ControlPersona</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletRA6</servlet-name>
        <url-pattern>/ServletRA6</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlMunicipio</servlet-name>
        <url-pattern>/ControlMunicipio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlPais</servlet-name>
        <url-pattern>/ControlPais</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlBD</servlet-name>
        <url-pattern>/ControlBD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlVivienda</servlet-name>
        <url-pattern>/ControlVivienda</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlPersona</servlet-name>
        <url-pattern>/ControlPersona</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <context-param>
 <param-name>tipoConexion</param-name>
 <param-value>3</param-value>
 </context-param>
</web-app>
